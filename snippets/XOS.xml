  <project name="android_packages_apps_Launcher3" path="packages/apps/Launcher3" remote="XOS" upstream="https://github.com/AOSiP/platform_packages_apps_Launcher3|ten" merge-aosp="true" />
  <project name="android_packages_apps_Messaging" path="packages/apps/Messaging" remote="XOS" upstream="https://github.com/AOSiP/platform_packages_apps_Messaging|ten" merge-aosp="true" />
  <project name="android_packages_apps_Nfc" path="packages/apps/Nfc" remote="XOS" upstream="https://github.com/AOSiP/platform_packages_apps_Nfc|ten" merge-aosp="true" />
  <project name="android_packages_apps_Settings"  path="packages/apps/Settings" remote="XOS" merge-aosp="true" />
  <project name="android_packages_apps_SettingsIntelligence" path="packages/apps/SettingsIntelligence" remote="XOS" merge-aosp="true" />
  <project name="android_packages_apps_ThemePicker" path="packages/apps/ThemePicker" remote="XOS" upstream="https://github.com/AOSiP/platform_packages_apps_ThemePicker|ten" merge-aosp="true" />
  <project name="android_packages_inputmethods_LatinIME" path="packages/inputmethods/LatinIME" remote="XOS" merge-aosp="true" />
  <project name="android_packages_providers_DownloadProvider" path="packages/providers/DownloadProvider" remote="XOS" merge-aosp="true" />
  <project name="android_packages_services_Telecomm" path="packages/services/Telecomm" remote="XOS" merge-aosp="true" />
