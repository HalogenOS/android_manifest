<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="XOS"
           fetch="https://git.halogenos.org/halogenOS"
           review="https://review.halogenos.org/"
           revision="refs/heads/XOS-10.0" />

  <!-- Bootable -->
  <project name="android_bootable_recovery" path="bootable/recovery" remote="XOS" merge-aosp="true" />

  <!-- Build -->
  <project name="android_build_make" path="build/make" remote="XOS" merge-aosp="true">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <project name="android_build_soong" path="build/soong" remote="XOS" merge-aosp="true">
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project name="android_device_aosip_sepolicy" path="device/aosip/sepolicy" remote="XOS" upstream="https://github.com/AOSiP/device_aosip_sepolicy|ten" />
  <project name="android_device_qcom_sepolicy" path="device/qcom/sepolicy" remote="XOS" upstream="https://github.com/AOSiP/device_qcom_sepolicy|ten" />

  <!-- External -->
  <project name="android_external_json-c" path="external/json-c" remote="XOS" merge-aosp="true" />
  <project name="android_external_selinux" path="external/selinux" remote="XOS" merge-aosp="true" upstream="https://github.com/AOSiP/platform_external_selinux|ten" />
  <project name="android_external_tinycompress" path="external/tinycompress" remote="XOS" merge-aosp="true" />
  <project name="android_external_xos" path="external/xos" remote="XOS"/>

  <!-- Frameworks -->
  <project name="android_frameworks_av" path="frameworks/av" remote="XOS" merge-aosp="true" />
  <project name="android_frameworks_base" path="frameworks/base" remote="XOS" merge-aosp="true" />

  <!-- Hardware -->
  <project name="android_hardware_custom_interfaces" path="hardware/custom/interfaces" remote="XOS" upstream="https://github.com/AOSiP/platform_hardware_aosip_interfaces|ten" />
  <project name="android_hardware_interfaces" path="hardware/interfaces" remote="XOS" merge-aosp="true" />
  <project name="android_hardware_libhardware" path="hardware/libhardware" remote="XOS" merge-aosp="true" />
  <project name="android_hardware_nxp_nfc" path="hardware/nxp/nfc" remote="XOS" merge-aosp="true" />

